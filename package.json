{
  "name": "webpack-vue",
  "version": "1.1.8",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "version": "# 修改 version  \n zx bump.mjs",
    "create": "node template/index.ts",
    "commit": "npm run version && npm run changelog && git add . && git cz ",
    "push": "npm run commit && git push origin HEAD",
    "start": "npx webpack-dev-server --mode=development --progress",
    "dev": "npx webpack-dev-server --mode=development --progress",
    "qa": "npx webpack-dev-server --progress --mode=development",
    "prod": "npx webpack-dev-server --mode=production --progress",
    "build:dev": "npx webpack --mode=production --progress",
    "build:qa": "npx webpack --mode=production --progress ",
    "build:prod": "npx webpack --mode=production --progress",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .jsx ./src --fix",
    "lint:style": "stylelint src/**/*.{scss,wxss} --fix",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,wxs}": [
      "prettier --config .prettierrc.js --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{wxss,scss}": [
      "prettier --config .prettierrc.js --write",
      "stylelint --fix --allow-empty-input",
      "git add"
    ]
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/parser": "^5.58.0",
    "ant-design-vue": "^3.2.17",
    "axios": "^1.3.5",
    "css-loader": "^6.7.3",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "pinia": "^2.0.34",
    "style-loader": "^3.3.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@statoscope/webpack-plugin": "^5.26.2",
    "@types/lodash": "^4.14.192",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-import": "^1.13.6",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.5.1",
    "commitlint-config-cz": "^0.13.3",
    "compression-webpack-plugin": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-minimizer-webpack-plugin": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "image-webpack-loader": "^8.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss-loader": "^7.2.4",
    "prettier": "^2.8.7",
    "prompts": "^2.4.2",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "raw-loader": "^4.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "terser-webpack-plugin": "^5.3.7",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "unplugin-vue-components": "^0.24.1",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^9.1.1",
    "vue-loader": "^17.0.1",
    "webpack-dev-server": "^4.13.2",
    "zx": "^7.2.1"
  }
}
