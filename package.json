{
  "name": "webpack-vue",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "version": "# 修改 version  \n zx bump.mjs",
    "create": "node template/index.ts",
    "commit": "npm run version && npm run changelog && git add . && git cz ",
    "push": "npm run commit && git push origin HEAD",
    "start": "npx webpack-dev-server --mode=development --progress",
    "dev": "npx webpack-dev-server --mode=development --progress",
    "qa": "npx webpack-dev-server --progress --mode=development",
    "prod": "npx webpack-dev-server --mode=production --progress",
    "build:dev": "npx webpack --mode=production --progress",
    "build:qa": "npx webpack --mode=production --progress ",
    "build:prod": "npx webpack --mode=production --progress",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .jsx ./src --fix",
    "lint:style": "stylelint src/**/*.{scss,wxss} --fix",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,wxs}": [
      "prettier --config .prettierrc.js --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{wxss,scss}": [
      "prettier --config .prettierrc.js --write",
      "stylelint --fix --allow-empty-input",
      "git add"
    ]
  },
  "dependencies": {
    "@types/node": "^20.8.6",
    "@typescript-eslint/parser": "^6.7.5",
    "ant-design-vue": "^4.0.3",
    "axios": "^1.5.1",
    "css-loader": "^6.8.1",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "style-loader": "^3.3.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@commitlint/config-conventional": "^17.8.0",
    "@statoscope/webpack-plugin": "^5.28.0",
    "@types/lodash": "^4.14.199",
    "@types/webpack-env": "^1.18.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.8.0",
    "commitlint-config-cz": "^0.13.3",
    "compression-webpack-plugin": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "html-minimizer-webpack-plugin": "^4.4.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "image-webpack-loader": "^8.1.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^15.0.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "prompts": "^2.4.2",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "raw-loader": "^4.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "unplugin-vue-components": "^0.25.2",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-loader": "^17.3.0",
    "webpack-dev-server": "^4.15.1",
    "zx": "^7.2.3"
  }
}
